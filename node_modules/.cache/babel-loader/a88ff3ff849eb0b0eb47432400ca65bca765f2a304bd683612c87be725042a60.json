     })[\"catch\"](function (err) {\n        console.log(err);\n      });\n    },\n    fillPage: function fillPage() {\n      this.curword = this.res[this.count];\n      // console.log(this.res)\n      this.word = this.res[this.count].word;\n      var numbers = [];\n      while (numbers.length < 4) {\n        var randomNumber = Math.floor(Math.random() * (9 + 1));\n        if (!numbers.includes(randomNumber) && this.res[randomNumber].word !== this.word) {\n          numbers.push(randomNumber);\n        }\n      }\n\n      // console.log(numbers)\n      for (var i = 0; i < 4; i++) {\n        this.prop[i].definition = this.res[numbers[i]].definition;\n        this.prop[i].word = this.res[numbers[i]].word;\n        this.prop[i].url = 'http://localhost:8082/' + 'output2/' + this.res[numbers[i]].definition_image;\n        this.prop[i].method = this.getOnError;\n      }\n      var j = Math.floor(Math.random() * 4);\n      this.prop[j].definition = this.res[this.count].definition;\n      this.prop[j].word = this.res[this.count].word;\n      this.prop[j].url = 'http://localhost:8082/' + 'output2/' + this.res[this.count].definition_image;\n      this.prop[j].method = this.getOnSuccess;\n    },\n    getOnSuccess: function getOnSuccess() {\n      var _this2 = this;\n      this.count++;\n      console.log('success');\n      if (this.count === 10) {\n        // console.log(\"total\",(parseInt(this.$store.state.userInfo.glossary[0])+1))\n        axios.get('/wordFinish/1/' + (parseInt(this.$store.state.userInfo.glossary[0]) + 1), {\n          headers: {\n            \"Authorization\": localStorage.getItem(\"token\")\n          }\n        }).then(function (res) {\n          // console.log(res)\n          if (res.data.status === 100) {\n            _this2.$message.success('完成');\n            _this2.refresh();\n          } else {\n            console.log(\"insert error\");\n          }\n        })[\"catch\"](function (err) {\n          console.log(err);\n        });\n      } else {\n        this.fillPage();\n      }\n    },\n    getOnError: function getOnError() {\n      // this.count ++\n      this.switchDetailWindow();\n      console.log('error');\n    },\n    highlightedPhrase: function highlightedPhrase() {\n      return this.curword.phrase.replace(new RegExp(this.curword.word, \"g\"), '<span class=\"highlight\">' + this.curword.word + '</span>');\n    },\n    switchDetailWindow: function switchDetailWindow() {\n      this.showDetailWindow = !this.showDetailWindow;\n    },\n    swishFinishWindow: function swishFinishWindow() {\n      this.showFinishWindow = !this.showFinishWindow;\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","showChatWindow","showDetailWindow","showFinishWindow","word","curword","res","count","audioSource","prop","definition","url","mounted","getData","props","methods","handlePlay","id","document","getElementById","setAttribute","play","toggleChatWindow","_this","refresh","get","parseInt","$store","state","userInfo","glossary","headers","token","then","fillPage","err","console","log","numbers","length","randomNumber","Math","floor","random","includes","push","i","definition_image","method","getOnError","j","getOnSuccess","_this2","localStorage","getItem","status","$message","success","switchDetailWindow","highlightedPhrase","phrase","replace","RegExp","swishFinishWindow"],"sources":["C:\\Users\\zhouy\\WebstormProjects\\lingo\\src\\view\\WordCard.vue"],"sourcesContent":["<template>\r\n  <div v-if=\"!showDetailWindow\" class=\"content home\">\r\n    <div class=\"left\">\r\n      <a-typography-title v-bind:content=\"word\"></a-typography-title>\r\n      <img class = \"sound\" @click=\"handlePlay('audio')\" src=\"../assets/sound.jpg\" alt=\"sound\"/>\r\n      <audio id=\"audio\"  hidden></audio>\r\n    </div>\r\n    <div class=\"right\">\r\n      <div class=\"card-grid\">\r\n        <div class=\"card\" :style=\"{ 'background-image': 'url(' + prop[0].url + ')','background-size': 'cover' }\" @click=\"prop[0].method\">\r\n          <a-typography-text class=\"definition\"  v-bind:content=\"prop[0].definition\"></a-typography-text>\r\n        </div>\r\n        <div class=\"card\" :style=\"{ 'background-image': 'url(' + prop[1].url + ')','background-size': 'cover' }\" @click=\"prop[1].method\">\r\n          <a-typography-text class=\"definition\" v-bind:content=\"prop[1].definition\"></a-typography-text>\r\n\r\n        </div>\r\n        <div class=\"card\" :style=\"{ 'background-image': 'url(' + prop[2].url + ')','background-size': 'cover' }\" @click=\"prop[2].method\">\r\n          <a-typography-text class=\"definition\" v-bind:content=\"prop[2].definition\"></a-typography-text>\r\n\r\n        </div>\r\n        <div class=\"card\" :style=\"{ 'background-image': 'url(' + prop[3].url + ')','background-size': 'cover' }\" @click=\"prop[3].method\">\r\n          <a-typography-text class=\"definition\" v-bind:content=\"prop[3].definition\"></a-typography-text>\r\n        </div>\r\n      </div>\r\n\r\n    </div>\r\n  </div>\r\n  <div v-if=\"showDetailWindow\" class=\"detail-window\">\r\n    <div class=\"left\">\r\n      <div>\r\n        <div class=\"word\">{{word}}</div>\r\n        <div>音标</div>\r\n      </div>\r\n      <div class=\"phrase\">\r\n        <div v-if=\"curword.phrase\">\r\n          <div v-html=\"highlightedPhrase(curword.phrase)\"></div>\r\n        </div>\r\n<!--        <div>{{highlightedPhrase(curword.phrase) }}</div>-->\r\n        <div>{{curword.phrase_explain}}</div>\r\n      </div>\r\n      <div>\r\n        <a-button @click=\"switchDetailWindow\">Next</a-button>\r\n      </div>\r\n    </div>\r\n    <div class=\"right\">\r\n      <div class=\"image\"\r\n           v-if=\"curword.phrase_image\"\r\n           :style=\"{ 'background-image': 'url(http://localhost:8082/output2/' + curword.phrase_image + ')', 'background-size': 'cover' }\">\r\n      </div>\r\n    </div>\r\n  </div>\r\n  <div v-if=\"showFinishWindow\" class=\"finish-window\">\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"@/axios\";\r\nexport default {\r\n  name: \"WordCard\",\r\n  data(){\r\n    return {\r\n      showChatWindow: false,\r\n      showDetailWindow: false,\r\n      showFinishWindow: false,\r\n      word: '',\r\n      curword:{},\r\n      res:{},\r\n      count:0,\r\n      audioSource:'https://api.vvhan.com/api/song?txt=',\r\n      prop: [\r\n        {definition:'',url:'',word:''},\r\n        {definition:'',url:'',word:''},\r\n        {definition:'',url:'',word:''},\r\n        {definition:'',url:'',word:''},\r\n      ]\r\n    }\r\n  },\r\n  mounted:function (){\r\n    this.getData()\r\n  },\r\n  props:['refresh'],\r\n  methods:{\r\n    handlePlay(id) {\r\n      document.getElementById(id).setAttribute('src',this.audioSource+this.curword.word)\r\n      document.getElementById(id).play()\r\n    },\r\n    toggleChatWindow() {\r\n      this.showChatWindow = !this.showChatWindow; // 切换窗口的显示和隐藏状态\r\n    },\r\n    getData(){\r\n      this.refresh()\r\n      axios.get('/wordApply/1/'+(parseInt(this.$store.state.userInfo.glossary[0])+1),{\r\n        headers: {\r\n          \"Authorization\": this.$store.state.token\r\n        }\r\n      }).then(res=>{\r\n        // console.log(res.data.data[this.count].definition_image)\r\n        this.res = res.data.data\r\n        this.fillPage()\r\n        // console.log(this.prop[0].url)\r\n      }).catch(err=>{\r\n        console.log(err)\r\n      })\r\n    },\r\n    fillPage(){\r\n      this.curword = this.res[this.count]\r\n      // console.log(this.res)\r\n      this.word = this.res[this.count].word\r\n      let numbers = [];\r\n\r\n      while (numbers.length < 4) {\r\n        let randomNumber = Math.floor(Math.random() * (9 + 1));\r\n        if (!numbers.includes(randomNumber)&&this.res[randomNumber].word !== this.word) {\r\n          numbers.push(randomNumber);\r\n        }\r\n      }\r\n\r\n      // console.log(numbers)\r\n      for(let i = 0;i<4;i++){\r\n        this.prop[i].definition = this.res[numbers[i]].definition\r\n        this.prop[i].word = this.res[numbers[i]].word\r\n        this.prop[i].url = 'http://localhost:8082/'+'output2/'+this.res[numbers[i]].definition_image\r\n        this.prop[i].method = this.getOnError\r\n      }\r\n      let j = Math.floor(Math.random() * 4);\r\n      this.prop[j].definition = this.res[this.count].definition\r\n      this.prop[j].word = this.res[this.count].word\r\n      this.prop[j].url = 'http://localhost:8082/'+'output2/'+this.res[this.count].definition_image\r\n      this.prop[j].method = this.getOnSuccess\r\n    },\r\n    getOnSuccess(){\r\n      this.count ++\r\n      console.log('success')\r\n      if(this.count === 10){\r\n        // console.log(\"total\",(parseInt(this.$store.state.userInfo.glossary[0])+1))\r\n        axios.get('/wordFinish/1/'+(parseInt(this.$store.state.userInfo.glossary[0])+1), {\r\n          headers: {\r\n            \"Authorization\": localStorage.getItem(\"token\")\r\n          }\r\n        }).then(res=>{\r\n          // console.log(res)\r\n          if(res.data.status === 100){\r\n            this.$message.success('完成')\r\n            this.refresh()\r\n          }else{\r\n            console.log(\"insert error\")\r\n          }\r\n        }).catch(err=>{\r\n          console.log(err)\r\n        })\r\n      }else{\r\n        this.fillPage()\r\n      }\r\n    },\r\n    getOnError(){\r\n      // this.count ++\r\n      this.switchDetailWindow()\r\n      console.log('error')\r\n    },\r\n    highlightedPhrase() {\r\n      return this.curword.phrase.replace(\r\n          new RegExp(this.curword.word, \"g\"),\r\n          '<span class=\"highlight\">' + this.curword.word + '</span>'\r\n      );\r\n    },\r\n    switchDetailWindow(){\r\n      this.showDetailWindow = !this.showDetailWindow\r\n    },\r\n    swishFinishWindow(){\r\n      this.showFinishWindow = !this.showFinishWindow\r\n    }\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n@import \"../scss/pages/_word-card.scss\";\r\n</