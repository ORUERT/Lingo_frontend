indow; // 切换窗口的显示和隐藏状态\r\n    },\r\n    getData(){\r\n      axios.get('/wordApply/1/'+this.$store.state.glossary[0]+1,{\r\n        headers: {\r\n          \"Authorization\": this.$store.state.token\r\n        }\r\n      }).then(res=>{\r\n        // console.log(res.data.data[this.count].definition_image)\r\n        this.res = res.data.data\r\n        this.fillPage()\r\n        // console.log(this.prop[0].url)\r\n      }).catch(err=>{\r\n        console.log(err)\r\n      })\r\n    },\r\n    fillPage(){\r\n      this.curword = this.res[this.count]\r\n      // console.log(this.res)\r\n      this.word = this.res[this.count].word\r\n      let numbers = [];\r\n\r\n      while (numbers.length < 4) {\r\n        let randomNumber = Math.floor(Math.random() * (9 + 1));\r\n        if (!numbers.includes(randomNumber)&&this.res[randomNumber].word !== this.word) {\r\n          numbers.push(randomNumber);\r\n        }\r\n      }\r\n\r\n      // console.log(numbers)\r\n      for(let i = 0;i<4;i++){\r\n        this.prop[i].definition = this.res[numbers[i]].definition\r\n        this.prop[i].word = this.res[numbers[i]].word\r\n        if(this.res[numbers[i]].definition_image === undefined){\r\n          console.log('word',this.res[numbers[i]].word)\r\n        }\r\n        this.prop[i].url = 'http://localhost:8082/'+'output2/'+this.res[numbers[i]].definition_image\r\n        this.prop[i].method = this.getOnError\r\n      }\r\n      let j = Math.floor(Math.random() * 4);\r\n      this.prop[j].definition = this.res[this.count].definition\r\n      this.prop[j].word = this.res[this.count].word\r\n      if(this.res[numbers[j]].definition_image === undefined){\r\n        console.log('word',this.res[numbers[j]].word)\r\n      }\r\n      this.prop[j].url = 'http://localhost:8082/'+'output2/'+this.res[this.count].definition_image\r\n      this.prop[j].method = this.getOnSuccess\r\n    },\r\n    getOnSuccess(){\r\n      this.count ++\r\n      console.log('success')\r\n      if(this.count === 10){\r\n        axios.get('/wordFinish/1/1', {\r\n          headers: {\r\n            \"Authorization\": localStorage.getItem(\"token\")\r\n          }\r\n        }).then(res=>{\r\n          // console.log(res)\r\n          if(res.data.status === 100){\r\n            this.$message.success('完成')\r\n          }else{\r\n            console.log(\"insert error\")\r\n          }\r\n        }).catch(err=>{\r\n          console.log(err)\r\n        })\r\n      }else{\r\n        this.fillPage()\r\n      }\r\n    },\r\n    getOnError(){\r\n      // this.count ++\r\n      this.switchDetailWindow()\r\n      console.log('error')\r\n    },\r\n    highlightedPhrase() {\r\n      return this.curword.phrase.replace(\r\n          new RegExp(this.curword.word, \"g\"),\r\n          '<span class=\"highlight\">' + this.curword.word + '</span>'\r\n      );\r\n    },\r\n    switchDetailWindow(){\r\n      this.showDetailWindow = !this.showDetailWindow\r\n    }\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n@import \"../scss/pages/_word-card.scss\";\r\n</style>"],"mappings":";;;;;;;;AAsDA,OAAOA,KAAI,MAAO,SAAS;AAC3B,SAASC,eAAc,QAAS,uBAAuB;AACvD,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAI,WAAAA,KAAA,EAAE;IACJ,OAAO;MACLC,cAAc,EAAE,KAAK;MACrBC,gBAAgB,EAAE,IAAI;MACtBC,IAAI,EAAE,EAAE;MACRC,OAAO,EAAC,CAAC,CAAC;MACVC,GAAG,EAAC,CAAC,CAAC;MACNC,KAAK,EAAC,CAAC;MACPC,IAAI,EAAE,CACJ;QAACC,UAAU,EAAC,EAAE;QAACC,GAAG,EAAC,EAAE;QAACN,IAAI,EAAC;MAAE,CAAC,EAC9B;QAACK,UAAU,EAAC,EAAE;QAACC,GAAG,EAAC,EAAE;QAACN,IAAI,EAAC;MAAE,CAAC,EAC9B;QAACK,UAAU,EAAC,EAAE;QAACC,GAAG,EAAC,EAAE;QAACN,IAAI,EAAC;MAAE,CAAC,EAC9B;QAACK,UAAU,EAAC,EAAE;QAACC,GAAG,EAAC,EAAE;QAACN,IAAI,EAAC;MAAE,CAAC;IAElC;EACF,CAAC;EACDO,OAAO,EAAC,SAAAA,QAAA,EAAW;IACjB,IAAI,CAACC,OAAO,CAAC;EACf,CAAC;EACDC,OAAO,EAAC;IACNC,gBAAgB,WAAAA,iBAAA,EAAG;MACjB,IAAI,CAACZ,cAAa,GAAI,CAAC,IAAI,CAACA,cAAc,EAAE;IAC9C,CAAC;IACDU,OAAO,WAAAA,QAAA,EAAE;MAAA,IAAAG,KAAA;MACPjB,KAAK,CAACkB,GAAG,CAAC,eAAe,GAAC,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,QAAQ,CAAC,CAAC,CAAC,GAAC,CAAC,EAAC;QACxDC