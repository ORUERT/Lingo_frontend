Register = ()=>{\r\n      console.log(\"helloworld\")\r\n    }\r\n    return {\r\n      formState,\r\n      rules,\r\n      onFinish,\r\n      onFinishFailed,\r\n      onRegister,\r\n    };\r\n  },\r\n});\r\n</script>\r\n\r\n<style scoped>\r\n  @import \"../scss/pages/_sign-in2.scss\";\r\n</style>"],"mappings":";AAmHA,SAASA,eAAe,EAAEC,QAAO,QAAS,KAAK;AAC/C,OAAOC,KAAI,MAAO,SAAS;AAC3B,SAASC,QAAO,QAAS,MAAM;AAC/B,SAAQC,SAAS,EAACC,QAAQ,QAAO,YAAY;AAE7C,eAAeL,eAAe,CAAC;EAC7BM,KAAK,WAAAA,MAAA,EAAG;IAAA,IAAAC,KAAA;IACN,IAAMC,KAAI,GAAIL,QAAQ,CAAC,CAAC;IACxB,IAAMM,MAAK,GAAIL,SAAS,CAAC,CAAC;IAC1B,IAAMM,UAAS,GAAIT,QAAQ,CAAC;MACzBU,KAAK,EAAC;IACT,CAAC,CAAC;IACF,IAAMC,SAAQ,GAAIX,QAAQ,CAAC;MACzBY,QAAQ,EAAE,QAAQ;MAClBC,QAAQ,EAAE,KAAK;MACfC,UAAU,EAAE;IACd,CAAC,CAAC;IACF,IAAMC,KAAI,GAAIf,QAAQ,CAAC;MACrBY,QAAQ,EAAE,CACR;QAAEI,QAAQ,EAAE,IAAI;QAAEC,OAAO,EAAE,QAAQ;QAAEC,OAAO,EAAE;MAAO,CAAC,EACtD;QAAEC,GAAG,EAAE,CAAC;QAAEC,GAAG,EAAE,EAAE;QAAEH,OAAO,EAAE,gBAAgB;QAAEC,OAAO,EAAE;MAAO,EAC/D;MACDL,QAAQ,EAAE,CACR;QAAEG,QAAQ,EAAE,IAAI;QAAEC,OAAO,EAAE,OAAO;QAAEC,OAAO,EAAE;MAAS;IAE1D,CAAC,CAAC;IACF,IAAMG,QAAO,GAAI,SAAXA,QAAOA,CAAIC,MAAK,EAAK;MACzBC,OAAO,CAACC,GAAG,CAAC,UAAU,EAAEF,MAAM,CAACV,QAAQ;MAEvCX,KAAK,CAACwB,IAAI,CAAC,aAAa,EAACH,MAAM,CAAC,CAACI,IAAI,CAAC,UAAAC,GAAG,EAAE;QACzC;QACAJ,OAAO,CAACC,GAAG,CAACG,GAAG,CAACC,IAAI;QACpB,IAAGD,GAAG,CAACC,IAAI,CAACC,IAAG,KAAM,KAAK,EAAC;UACzB,IAAMC,GAAE,GAAIH,GAAG,CAACC,IAAI,CAACA,IAAI;UACzBL,OAAO,CAACC,GAAG,CAAC,KAAK,EAACM,GAAG;;UAErB;UACAvB,KAAK,CAACwB,MAAM,CAAC,WAAW,EAAED,GAAG;UAC7B;UACAtB,MAAM,CAACwB,IAAI,CAAC,OAAO;QACrB,CAAC,MAAI;UACH1B,KAAI,CAAC2B,QAAQ,CAACC,KAAK,CAAC,MAAM;QAC5B;MACF,CAAC,CAAC,SAAM,CAAC,UAAAC,GAAG,EAAE;QACZZ,OAAO,CAACC,GAAG,CAACW,GAAG;MACjB,CAAC;IACH,CAAC;IACD,IAAMC,cAAa,GAAI,SAAjBA,cAAaA,CAAIC,SAAQ,EAAK;MAClCd,OAAO,CAACC,GAAG,CAAC,SAAS,EAAEa,SAAS,CAAC;IACnC,CAAC;IACD,IAAMC,UAAS,GAAI,SAAbA,UAASA,CAAA,EAAQ;MACrBf,OAAO,CAACC,GAAG,CAAC,YAAY;IAC1B;IACA,OAAO;MACLb,SAAS,EAATA,SAAS;MACTI,KAAK,EAALA,KAAK;MACLM,QAAQ,EAARA,QAAQ;MACRe,cAAc,EAAdA,